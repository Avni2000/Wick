{
  "name": "wick",
  "displayName": "Wick",
  "description": "Wick - Algorithmically Trade from VSCode",
  "version": "0.0.2",
  "publisher": "Avni2000",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Avni2000/Wick.git"
  },
  "bugs": {
    "url": "https://github.com/Avni2000/Wick/issues"
  },
  "homepage": "https://github.com/Avni2000/Wick#readme",
  "engines": {
    "vscode": "^1.106.1"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "wick.showChart",
        "title": "Wick: Show Chart"
      },
      {
        "command": "wick.openDashboard",
        "title": "Wick: Open Dashboard"
      }
    ],
    "viewsContainers": {
      "activitybar": [
        {
          "id": "wick-sidebar",
          "title": "Wick Trading",
          "icon": "$(graph)"
        }
      ]
    },
    "views": {
      "wick-sidebar": [
        {
          "id": "wick-main",
          "name": "Trading Dashboard"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "npm run check-types && npm run lint && node esbuild.js",
    "watch": "npm-run-all -p watch:*",
    "watch:esbuild": "node esbuild.js --watch",
    "watch:tsc": "tsc --noEmit --watch --project tsconfig.json",
    "package": "npm run check-types && npm run lint && node esbuild.js --production",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "npm run compile-tests && npm run compile && npm run lint",
    "check-types": "tsc --noEmit",
    "lint": "eslint src",
    "test": "vscode-test",
    "test:quick": "npm run compile-tests && npm run compile && vscode-test"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.10",
    "@types/node": "22.x",
    "@types/vscode": "^1.106.1",
    "@vscode/test-cli": "^0.0.12",
    "@vscode/test-electron": "^2.5.2",
    "esbuild": "^0.27.0",
    "eslint": "^9.39.1",
    "npm-run-all": "^4.1.5",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.46.3"
  },
  "dependencies": {
    "lightweight-charts": "^5.0.9"
  }
}